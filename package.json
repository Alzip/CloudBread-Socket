{
  "private": true,
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha tests/* --require babel-core/register --require ./tests/helper --recursive",
    "start": "$(npm bin)/babel-node tools/run dev",
    "build": "npm run test && $(npm bin)/babel-node tools/run build -- --release",
    "lint": "$(npm bin)/eslint ./src ./tools",
    "clean": "$(npm bin)/babel-node tools/run clean",
    "deploy": "npm run lint && npm run test && $(npm bin)/babel-node tools/run deploy -- --release"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3"
    ]
  },
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "bcrypt": "^0.8.5",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "kerberos": "0.0.17",
    "mongoose": "^4.3.6",
    "morgan": "^1.6.1",
    "shortid": "^2.2.4",
    "source-map-support": "^0.4.0",
    "validator": "^4.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "^6.4.3",
    "bluebird": "^3.1.1",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "file-loader": "^0.8.5",
    "gaze": "^0.5.2",
    "git-repository": "^0.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "ncp": "^2.0.0",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  }
}
