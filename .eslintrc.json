{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {
    "__DEV__": true,
    "__PORT__": true,
    "expect": true
  },
  "rules": {
    "quotes": [2, "single"],
    "curly": 2,
    "semi": [2, "always"],
    "new-cap": [
      2, {
        "capIsNewExceptions": ["Router"]
      }
    ]
  }
}
